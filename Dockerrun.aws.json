{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "hanyuanzhang/portfolio-nginx:test-0.0.2",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["angular"],
      "memory": 100
    },
    {
      "name": "angular",
      "image": "hanyuanzhang/portfolio-angular:test-0.0.2",
      "hostname": "angular",
      "essential": false,
      "memory": 300
    }
  ]
}
